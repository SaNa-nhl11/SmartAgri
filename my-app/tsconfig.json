{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.jsx",
    "app/index.jsx",
    "app/(tabs)/explore.jsx",
    "app/Home/_layout.jsx",
    "app/(tabs)/xlayout.jsx"
  ]
}
